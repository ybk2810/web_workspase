<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE 학생들 [
	<!ELEMENT 학생들 (학생)+>
	<!ELEMENT 학생 (학번, 이름+, 나이?)>
	<!ELEMENT 학번 (#PCDATA)>
	<!ELEMENT 이름 (#PCDATA)>
	<!ELEMENT 나이 (#PCDATA)>
	
	<!ATTLIST 학생 학년 (1|2|3|4) "1">
	<!ATTLIST 학번 학과 (컴공|작곡|수학|영문) "컴공">
]>
<!-- DTD
	+: 1개 이상
	*: 0개 이상
	?: 0 or 1개
	
	A|B: A또는 B
	(): 엘리먼트를 그룸으로 선언해서 사용
 -->
<학생들>
	<학생 학년="1">
		<학번 학과="컴공">18</학번>
		<이름>김유정</이름>
		<나이>20</나이>
	</학생>
	<학생 학년="1">
		<학번 학과="컴공">18</학번>
		<이름>이수현</이름>
		<이름>악동뮤지션</이름>
	</학생>
</학생들>